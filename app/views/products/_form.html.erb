<%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nameproduct %>
    <%= f.input :firmwareversion %>
    <%= f.input :descriptionproduct %>
    <%= f.input :active %>
    <%= f.input :ticket_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
