<%= simple_form_for(@ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :descriptionticket %>
    <%= f.input :priority %>
    <%= f.input :company %>
    <%= f.input :status %>
    <%= f.input :assignedto %>
    <%= f.input :contactname %>
    <%= f.input :user_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
